generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["jsonProtocol"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Repository {
  name       String?
  owner      String?
  team_name  String?
  cohort     String?
  demo_url   String?
  github_url String?
  region     String?
  statusId   Int?
  total_prs  Int?
  updated_at DateTime? @updatedAt
  id         Int       @id
  Status     Status?   @relation(fields: [statusId], references: [id])
}

model Status {
  id         Int          @id @default(autoincrement())
  status     String
  Repository Repository[]
}
